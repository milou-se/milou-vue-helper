{
  "name": "@milou.se/vue-helper",
  "version": "1.0.8",
  "description": "Helper functions for page based websites using VueJs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "npx karma start karma.conf.js --single-run",
    "docs": "npx typedoc --plugin typedoc-plugin-markdown"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/milou-se/milou-vue-helper.git"
  },
  "author": "Pontus Olofsson, Milou",
  "license": "ISC",
  "homepage": "https://github.com/milou-se/milou-vue-helper",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-dom": "^0.0.8",
    "@types/jquery": "^3.3.31",
    "@types/karma": "^3.0.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "jquery": "^3.4.1",
    "karma": "^4.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^4.1.1",
    "karma-typescript-es6-transform": "^4.1.1",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "puppeteer": "^1.20.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typescript": "^3.6.3",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "jquery": "^3.4.1",
    "vue": "^2.6.10"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/waldhow/MilouVueHelper/issues"
  },
  "directories": {
    "test": "test"
  }
}
